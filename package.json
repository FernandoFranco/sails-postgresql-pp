{
  "name": "sails-postgresql-pp",
  "description": "A postgreSQL adapter for Waterline and Sails.js based in sails-postgresql 0.11.4",
  "version": "1.0.0",
  "author": "Fernando Franco <atr.franco@gmail.com>",
  "url": "https://github.com/FernandoFranco/sails-postgresql-pp",
  "keywords": [
    "postgresql-pp",
    "postgresql",
    "orm",
    "waterline",
    "sails"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FernandoFranco/sails-postgresql-pp.git"
  },
  "dependencies": {
    "async": "1.5.2",
    "lodash": "3.10.1",
    "pg": "4.5.5",
    "waterline-cursor": "0.0.6",
    "waterline-errors": "0.10.1",
    "waterline-sequel": "0.5.7"
  },
  "devDependencies": {
    "captains-log": "0.11.11",
    "mocha": "2.4.5",
    "should": "8.2.1",
    "waterline-adapter-tests": "0.11.1"
  },
  "scripts": {
    "test": "make test",
    "docker": "docker-compose run adapter bash"
  },
  "main": "lib/adapter",
  "directories": {
    "lib": "./lib"
  },
  "license": "MIT",
  "bugs": "https://github.com/FernandoFranco/sails-postgresql-pp/issues",
  "waterlineAdapter": {
    "waterlineVersion": "~0.10.0",
    "interfaces": [
      "semantic",
      "queryable",
      "migratable",
      "associations",
      "sql"
    ],
    "features": [
      "crossAdapter",
      "unique",
      "autoIncrement",
      "schemas"
    ]
  }
}
